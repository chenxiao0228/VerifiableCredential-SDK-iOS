use_frameworks!

workspace '../sdk'

def sdk_pods
  pod 'SSZipArchive'
end

def networking_pods
  podspec :path => '../networking/networking.podspec'
end

target 'sdk' do
  project '../sdk/sdk.xcodeproj'
  sdk_pods
  networking_pods
end

target 'networking' do
  project '../networking/networking.xcodeproj'
  pod "PromiseKit", "~> 6.8"
  networking_pods
end
